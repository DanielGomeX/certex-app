            <RadioGroup
                android:id="@+id/rg_fix"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/devider1"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:paddingLeft="20dp">

                <RadioButton
                    android:id="@+id/cb_conforme_fix"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:button="@drawable/radiobutton_selector"
                    android:drawablePadding="20dp"
                    android:paddingLeft="10dip"
                    android:text="@string/radio_conforme" />

                <RadioButton
                    android:id="@+id/cb_nconforme_fix"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:button="@drawable/radiobutton_selector"
                    android:drawablePadding="20dp"
                    android:paddingLeft="10dip"
                    android:text="@string/radio_naoconforme" />

                <RadioButton
                    android:id="@+id/cb_na_fix"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:button="@drawable/radiobutton_selector"
                    android:drawablePadding="20dp"
                    android:paddingLeft="10dip"
                    android:text="@string/radio_na" />
            </RadioGroup>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/colorPrimaryDark"
                android:focusable="false" />

            <android.support.v4.widget.Space
                android:layout_width="match_parent"
                android:layout_height="10dp" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>